#include <iostream>
using namespace std;

int main()
{
	int a;
	cin >> a;

	int* array = new int[a];

	for (int i = 0; i < a; i++){
		cin >> array[i];
	}

	for (int i = 0, b = 0, c = 0, d = 0; i < 9 * a; b++, i++){

		if (array[b] == d) {
			c++;
		}

		if (b == a - 1) {
			if (c > 0) { cout << d << ":" << c << endl; }
			c = 0;
			d++;
			b = -1;
		}
	}

	delete[] array;
}
